# archived_from:
# registry-query01-DataServices.rq

PREFIX adms:  <http://www.w3.org/ns/adms#>
PREFIX dcat:  <http://www.w3.org/ns/dcat#>
PREFIX dct:   <http://purl.org/dc/terms/>
PREFIX foaf:  <http://xmlns.com/foaf/0.1/>
PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos:  <http://www.w3.org/2004/02/skos/core#>
PREFIX vcard: <http://www.w3.org/2006/vcard/ns#>
PREFIX edenr: <https://eden-fidelis.eu/example/registry/>


SELECT ?dataService ?title ?endpointURL ?country ?org_name {
    ?dataService a dcat:DataService;
                 dct:title ?title ;
                 dcat:endpointURL ?endpointURL ;
                 dct:publisher ?publisher .
    ?publisher vcard:country-name ?country ;
               foaf:name ?org_name
}
ORDER BY ?dataService